{"ast":null,"code":"import { useState, useEffect } from 'react';\nimport { useRouter } from 'next/router';\nimport { get } from 'lib/web';\nimport enUS from 'public/country/en-US.json';\nconst countryNames = {\n  'en-US': enUS\n};\nexport default function useCountryNames(locale) {\n  const {\n    0: list,\n    1: setList\n  } = useState(countryNames[locale] || enUS);\n  const {\n    basePath\n  } = useRouter();\n\n  async function loadData(locale) {\n    const {\n      ok,\n      data\n    } = await get(`${basePath}/country/${locale}.json`);\n\n    if (ok) {\n      countryNames[locale] = data;\n      setList(countryNames[locale]);\n    } else {\n      setList(enUS);\n    }\n  }\n\n  useEffect(() => {\n    if (!countryNames[locale]) {\n      loadData(locale);\n    } else {\n      setList(countryNames[locale]);\n    }\n  }, [locale]);\n  return list;\n}","map":null,"metadata":{},"sourceType":"module"}