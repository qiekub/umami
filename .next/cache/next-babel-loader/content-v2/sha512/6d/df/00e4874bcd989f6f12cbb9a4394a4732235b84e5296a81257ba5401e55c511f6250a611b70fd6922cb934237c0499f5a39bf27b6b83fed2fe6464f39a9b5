{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport classNames from 'classnames';\nimport styles from './Menu.module.css';\n\nfunction Menu({\n  options = [],\n  selectedOption,\n  className,\n  float,\n  align = 'left',\n  optionClassName,\n  selectedClassName,\n  onSelect = () => {}\n}) {\n  return /*#__PURE__*/_jsx(\"div\", {\n    className: classNames(styles.menu, className, {\n      [styles.float]: float,\n      [styles.top]: float === 'top',\n      [styles.bottom]: float === 'bottom',\n      [styles.left]: align === 'left',\n      [styles.right]: align === 'right'\n    }),\n    children: options.filter(({\n      hidden\n    }) => !hidden).map(option => {\n      const {\n        label,\n        value,\n        className: customClassName,\n        render,\n        divider\n      } = option;\n      return render ? render(option) : /*#__PURE__*/_jsx(\"div\", {\n        className: classNames(styles.option, optionClassName, customClassName, {\n          [selectedClassName]: selectedOption === option,\n          [styles.selected]: selectedOption === option,\n          [styles.divider]: divider\n        }),\n        onClick: e => onSelect(value, e),\n        children: label\n      }, value);\n    })\n  });\n}\n\nexport default Menu;","map":null,"metadata":{},"sourceType":"module"}