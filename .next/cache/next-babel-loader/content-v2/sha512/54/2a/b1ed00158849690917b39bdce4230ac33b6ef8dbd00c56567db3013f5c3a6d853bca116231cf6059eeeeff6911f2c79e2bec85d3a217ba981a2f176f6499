{"ast":null,"code":"export function removeTrailingSlash(url) {\n  return url && url.length > 1 && url.endsWith('/') ? url.slice(0, -1) : url;\n}\nexport function removeWWW(url) {\n  return url && url.length > 1 && url.startsWith('www.') ? url.slice(4) : url;\n}\nexport function getDomainName(str) {\n  try {\n    return new URL(str).hostname;\n  } catch (e) {\n    return str;\n  }\n}\nexport function getQueryString() {\n  var params = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : {};\n  var map = Object.keys(params).reduce(function (arr, key) {\n    if (params[key] !== undefined) {\n      return arr.concat(\"\".concat(key, \"=\").concat(encodeURIComponent(params[key])));\n    }\n\n    return arr;\n  }, []);\n\n  if (map.length) {\n    return \"?\".concat(map.join('&'));\n  }\n\n  return '';\n}\nexport function makeUrl(url, params) {\n  return \"\".concat(url).concat(getQueryString(params));\n}","map":null,"metadata":{},"sourceType":"module"}